rymysql(trymysql.drafts.text==request.args(0)).select()\n    table = []\n    biblio = [text.book]\n    biblio1=[]\n    other = []\n    seq = []\n    for all in variant:\n        filename2 = all.filename\n        book.append(all.book.short)\n        t2 = open(filename2, \'rb\')\n        text2 = t2.readlines()\n        # \xd1\x81\xd1\x80\xd0\xb0\xd0\xb2\xd0\xbd\xd0\xb8\xd0\xb2\xd0\xb0\xd0\xb5\xd0\xbc \xd1\x82\xd0\xb5\xd0\xba\xd1\x81\xd1\x82\xd1\x8b\n        d = difflib.Differ()\n        result = list(d.compare(text1, text2))\n        table.append(result)\n        biblio1.append(trymysql(trymysql.biblio.id==all.book).select()[0])\n#str(all.book.id) + \'. \'+ all.book.title + \'. \' + all.book.city + \': \' + all.book.editor + \', \' + all.book.year + \'. \' + all.book.part + ". C. " + all.book_page + \'.\')\n        n = {}\n        n[\'title\'] = all.title.upper()\n        n[\'dedication\'] = all.dedication\n        n[\'epi\'] = all.epi\n        n[\'epi_author\'] = all.epi_author\n        n[\'epi_book\'] = all.epi_book\n        other.append(n)\n    #for res in table:\n    count = 0\n    s = []\n    clear = [line for line in table[0] if not line.startswith(\'?\') and \'|\' not in line]\n    while len(clear)>0:\n        if clear[count].startswith(\'-\'):\n            next_line = clear[count+1][2:]\n            next_line = re.findallr"[\\w\']+|[.,!?;]", next_line, re.UNICODE)\n            cline = re.findall(r"[\\w\']+|[.,!?;]", clear[count][2:], re.UNICODE)\n            comp = difflib.Differ()\n            s1 = list(comp.compare(cline, next_line))\n            s.append(s1)\n            clear.remove(clear[count+1])\n        else:\n            s.append(clear[count][2:].split())\n            c = re.findall(r"\\w+|[^\\w\\s]", clear[count][2:], re.UNICODE)\n        clear.remove(clear[count])\n    return dict(book=book, table=table, text=text, biblio=biblio, biblio1=biblio1, other=other, s=s, cline=cline)\n\ndef show_variants(): # try to show xml\n    t = \'483\'\n    texts = t