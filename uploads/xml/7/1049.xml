      <stanza number="2">
        <line number="5">
          <word number="191506" pos="NOUN">Сил</word>
          <word number="191507" pos="CONJ">и</word>
          <word number="191508" pos="NOUN">дней</word>
          <word number="191509" pos="GRND">гордясь</word>
          <word number="191510" pos="NOUN">избытком</word>
          <word number="191511" pos="PNCT">,</word>
        </line>
        <line number="6">
          <word number="191512" pos="ADVB">Мимоходом</word>
          <word number="191513" pos="PNCT">,</word>
          <word number="191514" pos="PREP">на</word>
          <word number="191515" pos="NOUN">лету</word>
        </line>
        <line number="7">
          <word number="191516" pos="ADVB">Хмельно</word>
          <word number="191517" pos="PNCT">-</word>
          <word number="191518" pos="ADJF">розовым</word>
          <word number="191519" pos="NOUN">напитком</word>
        </line>
        <line number="8">
          <word number="191520" pos="INFN">Усыплять</word>
          <word number="191521" pos="ADJF">свою</word>
          <word number="191522" pos="NOUN">мечту</word>
          <word number="191523" pos="PNCT">.</word>
        </line>
      </stanza>
      <stanza number="3">
        <line number="9">
          <word number="191524" pos="GRND">Увидав</word>
          <word number="191525" pos="PNCT">,</word>
          <word number="191526" pos="CONJ">что</word>
          <word number="191527" pos="ADVB">невозможно</word>
        </line>
        <line number="10">
          <word number="191528" pos="PRCL">Ни</word>
          <word number="191529" pos="INFN">вернуться</word>
          <word number="191530" pos="PNCT">,</word>
          <word number="191531" pos="PRCL">ни</word>
          <word number="191532" pos="INFN">забыть</word>
          <word number="191533" pos="None">...</word>
        </line>
        <line number="11">
          <word number="191534" pos="INFN">Пить</word>
          <word number="191535" pos="ADVB">поспешно</word>
          <word number="191536" pos="PNCT">,</word>
          <word number="191537" pos="INFN">пить</word>
          <word number="191538" pos="ADJS">тревожно</word>
          <word number="191539" pos="PNCT">,</word>
        </line>
        <line number="12">
          <word number="191540" pos="NOUN">Рядом</word>
          <word number="191541" pos="PREP">с</word>
          <word number="191542" pos="NOUN">сыном</word>
          <word number="191543" pos="PNCT">,</word>
          <word number="191544" pos="VERB">может</word>
          <word number="191545" pos="INFN">быть</word>
          <word number="191546" pos="PNCT">,</word>
        </line>
      </stanza>
      <stanza number="4">
        <line number="13">
          <word number="191547" pos="PREP">Под</word>
          <word number="191548" pos="NOUN">наплывом</word>
          <word number="191549" pos="NOUN">лет</word>
          <word number="191550" pos="INFN">согнуться</word>
          <word number="191551" pos="PNCT">,</word>
        </line>
        <line number="14">
          <word number="191552" pos="CONJ">Но</word>
          <word number="191553" pos="PNCT">,</word>
          <word number="191554" pos="GRND">забыв</word>
          <word number="191555" pos="CONJ">и</word>
          <word number="191556" pos="NOUN">вкус</word>
          <word number="191557" pos="NOUN">вина</word>
          <word number="191558" pos="None">...</word>
        </line>
        <line number="15">
          <word number="191559" pos="PREP">По</word>
          <word number="191560" pos="NOUN">привычке</word>
          <word number="191561" pos="ADJF">всё</word>
          <word number="191562" pos="INFN">тянуться</word>
        </line>
        <line number="16">
          <word number="191563" pos="PREP">К</word>
          <word number="191564" pos="NOUN">чаше</word>
          <word number="191565" pos="PNCT">,</word>
          <word number="191566" pos="PRTF">выпитой</word>
          <word number="191567" pos="PREP">до</word>
          <word number="191568" pos="NOUN">дна</word>
          <word number="191569" pos="PNCT">.</word>
