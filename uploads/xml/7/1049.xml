        </line>
        <line number="2">
          <word number="191489" pos="CONJ">То</word>
          <word number="191490" pos="INFN">большиться</word>
          <word number="191491" pos="PNCT">,</word>
          <word number="191492" pos="CONJ">то</word>
          <word number="191493" pos="INFN">шалить</word>
          <word number="191494" pos="None">...</word>
        </line>
        <line number="3">
          <word number="191495" pos="CONJ">И</word>
          <word number="191496" pos="ADVB">рассеянно</word>
          <word number="191497" pos="PREP">из</word>
          <word number="191498" pos="NOUN">чаши</word>
        </line>
        <line number="4">
          <word number="191499" pos="NOUN">Пену</word>
          <word number="191500" pos="INFN">пить</word>
          <word number="191501" pos="PNCT">,</word>
          <word number="191502" pos="CONJ">а</word>
          <word number="191503" pos="NOUN">влагу</word>
          <word number="191504" pos="INFN">лить</word>
