  <text>
    <info>
      <title>Уже над морем вечереет...</title>
      <name>Александр</name>
      <author>Блок</author>
    </info>
    <poem>
      <stanza number="1">
        <line number="1">
          <word number="345309" pos="ADVB">Уже</word>
          <word number="345310" pos="PREP">над</word>
          <word number="345311" pos="NOUN">морем</word>
          <word number="345312" pos="VERB">вечереет</word>
          <word number="345313" pos="PNCT">,</word>
        </line>
        <line number="2">
          <word number="345314" pos="ADVB">Уж</word>
          <word number="345315" pos="NPRO">ты</word>
          <word number="345316" pos="NOUN">мечтой</word>
          <word number="345317" pos="NPRO">меня</word>
          <word number="345318" pos="VERB">томишь</word>
          <word number="345319" pos="PNCT">,</word>
        </line>
        <line number="3">
          <word number="345320" pos="CONJ">И</word>
          <word number="345321" pos="PREP">с</word>
          <word number="345322" pos="NOUN">полуночи</word>
          <word number="345323" pos="NOUN">ветер</word>
          <word number="345324" pos="VERB">веет</word>
        </line>
        <line number="4">
          <word number="345325" pos="PREP">Через</word>
          <word number="345326" pos="ADJF">неласковый</word>
          <word number="345327" pos="NOUN">камыш</word>
          <word number="345328" pos="PNCT">.</word>
        </line>
      </stanza>
      <stanza number="2">
        <line number="5">
          <word number="345329" pos="NOUN">Огни</word>
          <word number="345330" pos="PREP">на</word>
          <word number="345331" pos="NOUN">мачтах</word>
          <word number="345332" pos="GRND">зажигая</word>
          <word number="345333" pos="PNCT">,</word>
        </line>
        <line number="6">
          <word number="345334" pos="VERB">Уходят</word>
          <word number="345335" pos="PREP">в</word>
          <word number="345336" pos="NOUN">море</word>
          <word number="345337" pos="NOUN">корабли</word>
          <word number="345338" pos="PNCT">,</word>
        </line>
        <line number="7">
          <word number="345339" pos="CONJ">А</word>
          <word number="345340" pos="NPRO">ты</word>
          <word number="345341" pos="PNCT">,</word>
          <word number="345342" pos="ADJF">ночная</word>
          <word number="345343" pos="PNCT">,</word>
          <word number="345344" pos="NPRO">ты</word>
          <word number="345345" pos="PNCT">,</word>
          <word number="345346" pos="ADJF">земная</word>
          <word number="345347" pos="PNCT">,</word>
        </line>
        <line number="8">
          <word number="345348" pos="ADVB">Опять</word>
          <word number="345349" pos="VERB">уносишь</word>
          <word number="345350" pos="PREP">от</word>
          <word number="345351" pos="NOUN">земли</word>
          <word number="345352" pos="PNCT">.</word>
        </line>
      </stanza>
      <stanza number="3">
        <line number="9">
          <word number="345353" pos="NPRO">Ты</word>
          <word number="345354" pos="ADJF">вся</word>
          <word number="345355" pos="ADJS">пленительна</word>
          <word number="345356" pos="CONJ">и</word>
          <word number="345357" pos="ADJS">лжива</word>
          <word number="345358" pos="PNCT">,</word>
        </line>
        <line number="10">
          <word number="345359" pos="ADJF">Вся</word>
          <word number="345360" pos="PNCT">—</word>
          <word number="345361" pos="PREP">в</word>
          <word number="345362" pos="PRTF">отступающих</word>
          <word number="345363" pos="NOUN">огнях</word>
          <word number="345364" pos="PNCT">,</word>
        </line>
        <line number="11">
          <word number="345365" pos="PREP">Во</word>
          <word number="345366" pos="NOUN">мгле</word>
          <word number="345367" pos="ADJF">вечернего</word>
          <word number="345368" pos="NOUN">залива</word>
          <word number="345369" pos="PNCT">,</word>
        </line>
        <line number="12">
          <word number="345370" pos="PREP">В</word>
          <word number="345371" pos="ADVB">легко</word>
          <word number="345372" pos="PNCT">-</word>
          <word number="345373" pos="ADJF">туманных</word>
          <word number="345374" pos="NOUN">пеленах</word>
          <word number="345375" pos="PNCT">.</word>
        </line>
      </stanza>
      <stanza number="4">
        <line number="13">
          <word number="345376" pos="VERB">Позволь</word>
          <word number="345377" pos="CONJ">и</word>
          <word number="345378" pos="NPRO">мне</word>
          <word number="345379" pos="NOUN">огонь</word>
          <word number="345380" pos="ADJF">прибрежный</word>
        </line>
        <line number="14">
          <word number="345381" pos="NPRO">Тебе</word>
          <word number="345382" pos="ADVB">навстречу</word>
          <word number="345383" pos="INFN">развести</word>
          <word number="345384" pos="PNCT">,</word>
        </line>
        <line number="15">
          <word number="345385" pos="PREP">В</word>
          <word number="345386" pos="NOUN">венок</word>
          <word number="345387" pos="ADJF">страстной</word>
          <word number="345388" pos="CONJ">и</word>
          <word number="345389" pos="ADJF">неизбежный</word>
          <word number="345390" pos="PNCT">—</word>
        </line>
        <line number="16">
          <word number="345391" pos="NOUN">Цветок</word>
          <word number="345392" pos="NOUN">влюбленности</word>
          <word number="345393" pos="INFN">вплести</word>
          <word number="345394" pos="None">...</word>
        </line>
      </stanza>
      <stanza number="5">
        <line number="17">
          <word number="345395" pos="NOUN">Обетование</word>
          <word number="345396" pos="ADVB">неложно</word>
          <word number="345397" pos="None">:</word>
        </line>
        <line number="18">
          <word number="345398" pos="PREP">Передо</word>
          <word number="345399" pos="NPRO">мною</word>
          <word number="345400" pos="PNCT">—</word>
          <word number="345401" pos="NPRO">ты</word>
          <word number="345402" pos="ADVB">опять</word>
          <word number="345403" pos="PNCT">.</word>
        </line>
        <line number="19">
          <word number="345404" pos="NOUN">Душе</word>
          <word number="345405" pos="PRTF">влюбленной</word>
          <word number="345406" pos="ADVB">невозможно</word>
        </line>
        <line number="20">
          <word number="345407" pos="PREP">О</word>
          <word number="345408" pos="ADJF">сладкой</word>
          <word number="345409" pos="NOUN">смерти</word>
          <word number="345410" pos="PRCL">не</word>
          <word number="345411" pos="INFN">мечтать</word>
          <word number="345412" pos="PNCT">.</word>
        </line>
      </stanza>
    </poem>
  </text>
