  <text>
    <info>
      <title>Болотистым, пустынным лугом...</title>
      <name>Александр</name>
      <author>Блок</author>
    </info>
    <poem>
      <stanza number="1">
        <line number="1">
          <word number="348105" pos="ADJF">Болотистым</word>
          <word number="348106" pos="PNCT">,</word>
          <word number="348107" pos="ADJF">пустынным</word>
          <word number="348108" pos="NOUN">лугом</word>
        </line>
        <line number="2">
          <word number="348109" pos="VERB">Летим</word>
          <word number="348110" pos="PNCT">.</word>
          <word number="348111" pos="ADJF">Одни</word>
          <word number="348112" pos="PNCT">.</word>
        </line>
        <line number="3">
          <word number="348113" pos="ADVB">Вон</word>
          <word number="348114" pos="PNCT">,</word>
          <word number="348115" pos="ADVB">точно</word>
          <word number="348116" pos="NOUN">карты</word>
          <word number="348117" pos="PNCT">,</word>
          <word number="348118" pos="NOUN">полукругом</word>
        </line>
        <line number="4">
          <word number="348119" pos="VERB">Расходятся</word>
          <word number="348120" pos="NOUN">огни</word>
          <word number="348121" pos="PNCT">.</word>
        </line>
      </stanza>
      <stanza number="2">
        <line number="5">
          <word number="348122" pos="VERB">Гадай</word>
          <word number="348123" pos="PNCT">,</word>
          <word number="348124" pos="NOUN">дитя</word>
          <word number="348125" pos="PNCT">,</word>
          <word number="348126" pos="PREP">по</word>
          <word number="348127" pos="NOUN">картам</word>
          <word number="348128" pos="NOUN">ночи</word>
          <word number="348129" pos="PNCT">,</word>
        </line>
        <line number="6">
          <word number="348130" pos="ADVB">Где</word>
          <word number="348131" pos="ADJF">твой</word>
          <word number="348132" pos="NOUN">маяк</word>
          <word number="348133" pos="None">...</word>
        </line>
        <line number="7">
          <word number="348134" pos="PRCL">Еще</word>
          <word number="348135" pos="COMP">смелей</word>
          <word number="348136" pos="NPRO">нам</word>
          <word number="348137" pos="VERB">хлынет</word>
          <word number="348138" pos="PREP">в</word>
          <word number="348139" pos="NOUN">очи</word>
        </line>
        <line number="8">
          <word number="348140" pos="ADJF">Неотвратимый</word>
          <word number="348141" pos="NOUN">мрак</word>
          <word number="348142" pos="PNCT">.</word>
        </line>
      </stanza>
      <stanza number="3">
        <line number="9">
          <word number="348143" pos="NPRO">Он</word>
          <word number="348144" pos="NOUN">морем</word>
          <word number="348145" pos="ADVB">ночью</word>
          <word number="348146" pos="ADJS">замкнут</word>
          <word number="348147" pos="PNCT">—</word>
          <word number="348148" pos="ADJF">дальный</word>
        </line>
        <line number="10">
          <word number="348149" pos="NOUN">Простор</word>
          <word number="348150" pos="NOUN">лугов</word>
          <word number="348151" pos="PNCT">!</word>
        </line>
        <line number="11">
          <word number="348152" pos="CONJ">И</word>
          <word number="348153" pos="NOUN">запах</word>
          <word number="348154" pos="ADJF">горький</word>
          <word number="348155" pos="CONJ">и</word>
          <word number="348156" pos="ADJF">печальный</word>
        </line>
        <line number="12">
          <word number="348157" pos="NOUN">Туманов</word>
          <word number="348158" pos="CONJ">и</word>
          <word number="348159" pos="NOUN">духов</word>
          <word number="348160" pos="PNCT">,</word>
        </line>
      </stanza>
      <stanza number="4">
        <line number="13">
          <word number="348161" pos="CONJ">И</word>
          <word number="348162" pos="NOUN">кольца</word>
          <word number="348163" pos="PREP">сквозь</word>
          <word number="348164" pos="NOUN">перчатки</word>
          <word number="348165" pos="ADJF">тонкой</word>
          <word number="348166" pos="PNCT">,</word>
        </line>
        <line number="14">
          <word number="348167" pos="CONJ">И</word>
          <word number="348168" pos="ADJF">строгий</word>
          <word number="348169" pos="NOUN">вид</word>
          <word number="348170" pos="PNCT">,</word>
        </line>
        <line number="15">
          <word number="348171" pos="CONJ">И</word>
          <word number="348172" pos="PRCL">это</word>
          <word number="348173" pos="PREP">над</word>
          <word number="348174" pos="NOUN">пустыней</word>
          <word number="348175" pos="ADJF">звонкой</word>
        </line>
        <line number="16">
          <word number="348176" pos="PREP">От</word>
          <word number="348177" pos="NOUN">цоканья</word>
          <word number="348178" pos="NOUN">копыт</word>
          <word number="348179" pos="PNCT">—</word>
        </line>
      </stanza>
      <stanza number="5">
        <line number="17">
          <word number="348180" pos="None">Все</word>
          <word number="348181" pos="VERB">говорит</word>
          <word number="348182" pos="PREP">о</word>
          <word number="348183" pos="ADJF">беспредельном</word>
          <word number="348184" pos="PNCT">.</word>
        </line>
        <line number="18">
          <word number="348185" pos="None">Все</word>
          <word number="348186" pos="VERB">хочет</word>
          <word number="348187" pos="NPRO">нам</word>
          <word number="348188" pos="INFN">помочь</word>
          <word number="348189" pos="PNCT">,</word>
        </line>
        <line number="19">
          <word number="348190" pos="CONJ">Как</word>
          <word number="348191" pos="ADJF">этот</word>
          <word number="348192" pos="NOUN">мир</word>
          <word number="348193" pos="PNCT">,</word>
          <word number="348194" pos="INFN">лететь</word>
          <word number="348195" pos="ADVB">бесцельно</word>
        </line>
        <line number="20">
          <word number="348196" pos="PREP">В</word>
          <word number="348197" pos="PRTF">сияющую</word>
          <word number="348198" pos="NOUN">ночь</word>
          <word number="348199" pos="PNCT">!</word>
        </line>
      </stanza>
    </poem>
