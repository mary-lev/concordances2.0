  <text>
    <info>
      <title>На темном небе, как узор...</title>
      <name>Осип</name>
      <author>Мандельштам</author>
    </info>
    <poem>
      <stanza number="1">
        <line number="1">
          <word number="55896" pos="PREP">На</word>
          <word number="55897" pos="ADJF">темном</word>
          <word number="55898" pos="NOUN">небе</word>
          <word number="55899" pos="PNCT">,</word>
          <word number="55900" pos="CONJ">как</word>
          <word number="55901" pos="NOUN">узор</word>
          <word number="55902" pos="PNCT">,</word>
        </line>
        <line number="2">
          <word number="55903" pos="NOUN">Деревья</word>
          <word number="55904" pos="ADJF">траурные</word>
          <word number="55905" pos="PRTS">вышиты</word>
          <word number="55906" pos="PNCT">.</word>
        </line>
        <line number="3">
          <word number="55907" pos="ADVB">Зачем</word>
          <word number="55908" pos="PRCL">же</word>
          <word number="55909" pos="ADVB">выше</word>
          <word number="55910" pos="CONJ">и</word>
          <word number="55911" pos="ADJF">все</word>
          <word number="55912" pos="ADVB">выше</word>
          <word number="55913" pos="NPRO">ты</word>
        </line>
        <line number="4">
          <word number="55914" pos="VERB">Возводишь</word>
          <word number="55915" pos="ADJF">изумленный</word>
          <word number="55916" pos="NOUN">взор</word>
          <word number="55917" pos="PNCT">?</word>
        </line>
      </stanza>
      <stanza number="2">
        <line number="5">
          <word number="55918" pos="PNCT">—</word>
          <word number="55919" pos="ADVB">Вверху</word>
          <word number="55920" pos="PNCT">—</word>
          <word number="55921" pos="ADJF">такая</word>
          <word number="55922" pos="NOUN">темнота</word>
          <word number="55923" pos="PNCT">,</word>
          <word number="55924" pos="PNCT">—</word>
        </line>
        <line number="6">
          <word number="55925" pos="NPRO">Ты</word>
          <word number="55926" pos="VERB">скажешь</word>
          <word number="55927" pos="PNCT">,</word>
          <word number="55928" pos="PNCT">—</word>
          <word number="55929" pos="NOUN">время</word>
          <word number="55930" pos="VERB">опрокинула</word>
        </line>
        <line number="7">
          <word number="55931" pos="CONJ">И</word>
          <word number="55932" pos="PNCT">,</word>
          <word number="55933" pos="CONJ">словно</word>
          <word number="55934" pos="NOUN">ночь</word>
          <word number="55935" pos="PNCT">,</word>
          <word number="55936" pos="PREP">на</word>
          <word number="55937" pos="NOUN">день</word>
          <word number="55938" pos="VERB">нахлынула</word>
        </line>
        <line number="8">
          <word number="55939" pos="NOUN">Холмов</word>
          <word number="55940" pos="ADJF">холодная</word>
          <word number="55941" pos="NOUN">черта</word>
          <word number="55942" pos="PNCT">.</word>
        </line>
      </stanza>
      <stanza number="3">
        <line number="9">
          <word number="55943" pos="ADJF">Высоких</word>
          <word number="55944" pos="PNCT">,</word>
          <word number="55945" pos="ADJF">неживых</word>
          <word number="55946" pos="NOUN">дерев</word>
        </line>
        <line number="10">
          <word number="55947" pos="PRTF">Темнеющее</word>
          <word number="55948" pos="VERB">рвется</word>
          <word number="55949" pos="NOUN">кружево</word>
          <word number="55950" pos="PNCT">:</word>
        </line>
        <line number="11">
          <word number="55951" pos="PREP">О</word>
          <word number="55952" pos="PNCT">,</word>
          <word number="55953" pos="NOUN">месяц</word>
          <word number="55954" pos="PNCT">,</word>
          <word number="55955" pos="PRCL">только</word>
          <word number="55956" pos="NPRO">ты</word>
          <word number="55957" pos="PRCL">не</word>
          <word number="55958" pos="VERB">суживай</word>
        </line>
        <line number="12">
          <word number="55959" pos="NOUN">Серпа</word>
          <word number="55960" pos="PNCT">,</word>
          <word number="55961" pos="ADVB">внезапно</word>
          <word number="55962" pos="GRND">почернев</word>
          <word number="55963" pos="PNCT">!</word>
        </line>
