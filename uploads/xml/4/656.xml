          <word number="68040" pos="NOUN">зданий</word>
          <word number="68041" pos="ADJF">красная</word>
          <word number="68042" pos="NOUN">подкова</word>
          <word number="68043" pos="PNCT">;</word>
        </line>
        <line number="11">
          <word number="68044" pos="CONJ">Как</word>
          <word number="68045" pos="CONJ">будто</word>
          <word number="68046" pos="VERB">слышу</word>
          <word number="68047" pos="NPRO">я</word>
          <word number="68048" pos="PREP">в</word>
          <word number="68049" pos="ADJF">октябрьский</word>
          <word number="68050" pos="ADJF">тусклый</word>
