      </stanza>
      <stanza number="5">
        <line number="17">
          <word number="68083" pos="CONJ">И</word>
          <word number="68084" pos="CONJ">если</word>
          <word number="68085" pos="PREP">для</word>
          <word number="68086" pos="ADJF">других</word>
          <word number="68087" pos="ADJF">восторженный</word>
          <word number="68088" pos="NOUN">народ</word>
        </line>
        <line number="18">
          <word number="68089" pos="NOUN">Венки</word>
          <word number="68090" pos="VERB">свивает</word>
          <word number="68091" pos="ADJF">золотые</word>
          <word number="68092" pos="PNCT">,</word>
          <word number="68093" pos="PNCT">—</word>
        </line>
        <line number="19">
          <word number="68094" pos="INFN">Благословить</word>
          <word number="68095" pos="NPRO">тебя</word>
          <word number="68096" pos="PREP">в</word>
          <word number="68097" pos="ADJF">далекий</word>
          <word number="68098" pos="NOUN">ад</word>
          <word number="68099" pos="VERB">сойдет</word>
        </line>
        <line number="20">
          <word number="68100" pos="NOUN">Стопами</word>
          <word number="68101" pos="ADJF">легкими</word>
          <word number="68102" pos="NOUN">Россия</word>
          <word number="68103" pos="PNCT">.</word>
