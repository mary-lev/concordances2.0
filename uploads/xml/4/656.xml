          <word number="67995" pos="NOUN">злобы</word>
        </line>
        <line number="3">
          <word number="67996" pos="CONJ">И</word>
          <word number="67997" pos="VERB">ощетинился</word>
          <word number="67998" pos="NOUN">убийца</word>
          <word number="67999" pos="PNCT">-</word>
          <word number="68000" pos="NOUN">броневик</word>
          <word number="68001" pos="PNCT">,</word>
        </line>
        <line number="4">
          <word number="68002" pos="CONJ">И</word>
          <word number="68003" pos="NOUN">пулеметчик</word>
          <word number="68004" pos="ADJF">низколобый</word>
          <word number="68005" pos="PNCT">,</word>
          <word number="68006" pos="PNCT">—</word>
        </line>
      </stanza>
      <stanza number="2">
        <line number="5">
          <word number="68007" pos="PNCT">—</word>
          <word number="68008" pos="NOUN">Керенского</word>
          <word number="68009" pos="INFN">распять</word>
          <word number="68010" pos="PNCT">!</word>
          <word number="68011" pos="PNCT">—</word>
          <word number="68012" pos="VERB">потребовал</word>
          <word number="68013" pos="NOUN">солдат</word>
          <word number="68014" pos="PNCT">,</word>
        </line>
        <line number="6">
          <word number="68015" pos="CONJ">И</word>
          <word number="68016" pos="ADJF">злая</word>
          <word number="68017" pos="NOUN">чернь</word>
          <word number="68018" pos="VERB">рукоплескала</word>
          <word number="68019" pos="PNCT">:</word>
        </line>
        <line number="7">
          <word number="68020" pos="NPRO">Нам</word>
          <word number="68021" pos="NOUN">сердце</word>
          <word number="68022" pos="PREP">на</word>
          <word number="68023" pos="NOUN">штыки</word>
          <word number="68024" pos="VERB">позволил</word>
          <word number="68025" pos="INFN">взять</word>
          <word number="68026" pos="NOUN">Пилат</word>
          <word number="68027" pos="PNCT">,</word>
        </line>
        <line number="8">
          <word number="68028" pos="CONJ">И</word>
          <word number="68029" pos="NOUN">сердце</word>
          <word number="68030" pos="INFN">биться</word>
          <word number="68031" pos="VERB">перестало</word>
          <word number="68032" pos="PNCT">!</word>
        </line>
      </stanza>
      <stanza number="3">
        <line number="9">
          <word number="68033" pos="CONJ">И</word>
          <word number="68034" pos="ADVB">укоризненно</word>
          <word number="68035" pos="VERB">мелькает</word>
          <word number="68036" pos="ADJF">эта</word>
          <word number="68037" pos="NOUN">тень</word>
          <word number="68038" pos="PNCT">,</word>
        </line>
