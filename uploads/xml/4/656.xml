          <word number="68055" pos="NPRO">его</word>
          <word number="68056" pos="PNCT">,</word>
          <word number="68057" pos="NOUN">щенка</word>
          <word number="68058" pos="NOUN">Петрова</word>
          <word number="68059" pos="PNCT">!</word>
        </line>
      </stanza>
      <stanza number="4">
        <line number="13">
          <word number="68060" pos="PREP">Среди</word>
          <word number="68061" pos="ADJF">гражданских</word>
          <word number="68062" pos="NOUN">бурь</word>
          <word number="68063" pos="CONJ">и</word>
          <word number="68064" pos="ADJF">яростных</word>
          <word number="68065" pos="NOUN">личин</word>
          <word number="68066" pos="PNCT">,</word>
        </line>
        <line number="14">
          <word number="68067" pos="ADJF">Тончайшим</word>
          <word number="68068" pos="NOUN">гневом</word>
          <word number="68069" pos="GRND">пламенея</word>
          <word number="68070" pos="PNCT">,</word>
        </line>
        <line number="15">
          <word number="68071" pos="NPRO">Ты</word>
          <word number="68072" pos="VERB">шел</word>
          <word number="68073" pos="ADVB">бестрепетно</word>
          <word number="68074" pos="PNCT">,</word>
          <word number="68075" pos="ADJF">свободный</word>
          <word number="68076" pos="NOUN">гражданин</word>
          <word number="68077" pos="PNCT">,</word>
        </line>
        <line number="16">
          <word number="68078" pos="ADVB">Куда</word>
          <word number="68079" pos="VERB">вела</word>
          <word number="68080" pos="NPRO">тебя</word>
          <word number="68081" pos="NOUN">Психея</word>
          <word number="68082" pos="PNCT">.</word>
