{{extend 'layout.html'}}
<h1>Сравнение со списком: {{=text.title}}</h1>

{{result1=[]}}
{{result2 = []}}

<table>
    {{for all in table:}}
    <th>{{=all[0][0]}} {{=all[0][1]}}</th> {{pass}}
        {{for x in range(0, 786):}}
            <tr> {{for a in range(len(table)):}}
                <td>
                    {{for aa in table[a][x]:}}
                    {{if aa.startswith('?'):}}

                    {{elif aa.startswith('+'):}}
                        <strike>{{=aa[2:]}}</strike> {{if '.' not in aa and ',' not in aa: }} {{result1.append(aa)}} {{pass}}
                    {{elif aa.startswith('-'):}}
                        <i>{{=aa[2:]}}</i> {{if '.' not in aa and ',' not in aa: }}  {{result2.append(aa)}} {{pass}}
                    {{else:}}
                        {{=aa}}
                    {{pass}}{{pass}}
                </td>  {{pass}}
        </tr>{{pass}}
</table>

<table>
    <tr><td> {{for all in result1:}} {{=all}}<br> {{pass}}</td> <td> {{for all in result2:}} {{=all}} <br>{{pass}}</td> </tr>

    </table>
